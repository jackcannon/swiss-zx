{
  "name": "swiss-zx",
  "version": "2.0.0",
  "author": "Jack Cannon <jackc@annon.co.uk> (http://c.annon.co.uk/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackcannon/swiss-zx.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "jest": "^29.6.4",
    "swiss-docs": "^0.7.4",
    "ts-jest": "^29.1.1",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "yarn build:jsdoc && yarn build:code && yarn docs",
    "quickbuild": "yarn build:code && yarn docs",
    "build:jsdoc": "swiss-docs --src src --jsdoc",
    "build:code": "tsup-node src/index.ts --format cjs,esm --dts --clean",
    "watch": "yarn build -- --watch src",
    "docs": "swiss-docs --src src --output README.md --alias dist/index.d.ts",
    "test": "yarn quickbuild && yarn test:raw",
    "test:raw": "node --experimental-vm-modules ./node_modules/.bin/jest --no-cache",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "fuse.js": "^6.6.2",
    "prompts": "^2.4.2",
    "string-width": "^5.1.2",
    "swiss-ak": "^2.2.1",
    "swiss-node": "^1.3.1",
    "zx": "^7.2.3"
  }
}
